<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.springboot.entity.OrderEntity" table="orders" schema="bookstore">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="submittime">
            <column name="submittime" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="bookname">
            <column name="bookname" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="amount">
            <column name="amount" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="price">
            <column name="price" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="sum">
            <column name="sum" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>